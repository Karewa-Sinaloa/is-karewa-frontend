root: true
env:
  browser: true
  es6: true
parser: vue-eslint-parser
extends:
  - plugin:prettier/recommended
  - prettier
  - plugin:vue/vue3-recommended
  - plugin:vue/vue3-essentials
plugins:
  - vue
  - prettier
globals:
  Atomics: readonly
  SharedArrayBuffer: readonly
parserOptions:
  ecmaVersion: 2018
  sourceType: module
